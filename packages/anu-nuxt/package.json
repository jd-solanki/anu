    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anu-vue/preset-theme-default": "workspace:*",
    "@iconify-json/bx": "latest",
    "@nuxt/kit": "^3.0.0",
    "anu-vue": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@unocss/nuxt": "^0.48.0",
    "@unocss/preset-icons": "^0.47.6",
