{
  "name": "@anu-vue/core",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "description": "DX focused utility based component library",
  "author": "JD Solanki <https://github.com/jd-solanki>",
  "license": "ISC",
  "funding": "https://github.com/sponsors/jd-solanki",
  "homepage": "https://anu-vue.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jd-solanki/anu.git",
    "directory": "packages/anu-vue"
  },
  "bugs": {
    "url": "https://github.com/jd-solanki/anu/issues"
  },
  "keywords": [
    "component library",
    "ui components",
    "vuejs",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/anu-vue.js",
      "require": "./dist/anu-vue.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/anu-vue.umd.cjs",
  "module": "./dist/anu-vue.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "scripts": {
    "dev": "pnpm gen-volar && concurrently \"vite build --watch\" \"vue-tsc --declaration --emitDeclarationOnly --noEmit false --watch\"",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --noEmit false && pnpm gen-volar",
    "gen-volar": "na tsx ../../scripts/gen-volar.ts"
  },
  "dependencies": {
    "deepmerge-ts": "^5.1.0",
    "defu": "^6.1.4",
    "radix-vue": "^1.3.2",
    "vue": "^3.4.14"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.11.5",
    "@vueuse/core": "^10.7.2",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  }
}
