    "lint": "pnpm --filter anu-vue lint",
    "update-deps": "taze -r -w --install",
    "update-deps:major": "taze major -r -w --install",
    "test": "nr --filter anu-vue test",
    "test-ui": "nr --filter anu-vue test-ui"
  },
